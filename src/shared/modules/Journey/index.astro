---
import Section from "../../components/Section/index.astro";
import Title from "../../components/Title/index.astro";
import Journal from "./Journal.astro";
import data from "../../data/journeyData.json";
---

<Section title="journey">
  <Title>
    <i class="fa-solid fa-compass"></i>Journey
  </Title>
  <ul class="flex flex-col mt-8">
    {
      data.map((d) => {
        if (d.id !== 2) return <Journal data={d} />;
      })
    }
  </ul>
</Section>
